{
  "name": "@quizzme/api-contracts",
  "version": "1.0.0",
  "description": "Shared API contracts and types for QuizzMe",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.mjs",
      "require": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "quizzme",
    "api",
    "contracts",
    "types",
    "typescript"
  ],
  "author": "QuizzMe Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
